<?xml version="1.0"?>
<cd:ProductDefinition xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10" id="E___FCRT__" format="xml" last-modified="2013-11-04">
  <cd:Record format="xml">
    <cd:Field name="Earth_Explorer_File">
      <cd:Record namexml="Earth_Explorer_File">
        <cd:Field name="Earth_Explorer_Header">
          <cd:Record namexml="Earth_Explorer_Header">
            <cd:Description>Earth Explorer Header File</cd:Description>
            <cd:Field name="Fixed_Header">
              <cd:NamedType id="Fixed_Header"/>
            </cd:Field>
            <cd:Field name="Variable_Header">
              <cd:Record namexml="Variable_Header">
                <cd:Field name="Configuration_Identifier">
                  <cd:Type namexml="Configuration_Identifier">
                    <cd:Integer format="ascii">
                      <cd:NativeType>uint32</cd:NativeType>
                      <ct:NamedTest id="RangeConfigurationIdentifier"/>
                    </cd:Integer>
                  </cd:Type>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <ct:NamedTest id="FileNameCheckEOF" path="Fixed_Header/File_Name"/>
            <ct:NamedTest id="FileNameMatchSystem" path="Fixed_Header/File_Name"/>
            <ct:NamedTest id="FileNameMatchCreationDate" path="Fixed_Header/File_Name"/>
            <ct:NamedTest id="FileNameMatchInstrumentConfigId" path="Fixed_Header/File_Name"/>
            <ct:NamedTest id="FixedValueSentinel1x" path="Fixed_Header/Mission"/>
          </cd:Record>
        </cd:Field>
        <cd:Field name="Data_Block">
          <cd:Record namexml="Data_Block">
            <cd:Attribute name="type">
              <cd:FixedValue>xml</cd:FixedValue>
            </cd:Attribute>
            <cd:Field name="RxM_Filter_Control_Register_Table">
              <cd:Record namexml="RxM_Filter_Control_Register_Table">
                <cd:Field name="FCRT_Entry">
                  <cd:Array>
                    <cd:Dimension/>
                    <cd:Record namexml="FCRT_Entry">
                      <cd:Field name="Spare">
                        <cd:Type namexml="Spare">
                          <cd:Integer format="ascii">
                            <cd:NativeType>uint8</cd:NativeType>
                            <ct:NamedTest id="FixedValue0"/>
                          </cd:Integer>
                        </cd:Type>
                      </cd:Field>
                      <cd:Field name="Filter_Number">
                        <cd:Type namexml="Filter_Number">
                          <cd:Integer format="ascii">
                            <cd:NativeType>uint8</cd:NativeType>
                            <ct:NamedTest id="RangeFilterNumber"/>
                          </cd:Integer>
                        </cd:Type>
                      </cd:Field>
                      <cd:Field name="Filter_Control_Register">
                        <cd:Type namexml="Filter_Control_Register">
                          <cd:Integer format="ascii">
                            <cd:NativeType>uint16</cd:NativeType>
                            <ct:NamedTest id="RangeConfigurationIdentifier"/>
                          </cd:Integer>
                        </cd:Type>
                      </cd:Field>
                    </cd:Record>
                    <ct:NamedTest id="RangeFCRTEntryCount"/>
                  </cd:Array>
                </cd:Field>
              </cd:Record>
            </cd:Field>
          </cd:Record>
        </cd:Field>
      </cd:Record>
    </cd:Field>
  </cd:Record>
</cd:ProductDefinition>
